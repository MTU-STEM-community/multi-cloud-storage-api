generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model File {
    id             String         @id @default(cuid())
    name           String
    size           Int
    type           String
    cloudLocations String[]
    createdAt      DateTime       @default(now())
    updatedAt      DateTime       @updatedAt
    cloudStorage   CloudStorage[]
}

model CloudStorage {
    id        String   @id @default(cuid())
    provider  String
    apiKey    String
    files     File[]
    createdAt DateTime @default(now())
}
